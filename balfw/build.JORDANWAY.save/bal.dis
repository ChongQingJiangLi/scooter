read_encoders(uint32_t *enc1_r, uint32_t *enc2_r)
{
  set0_enc_sel1();
  set0_enc_oe1_not();
  _delay_loop_1(10);
  uint8_t enc1h = getpin_enc_data();
  set1_enc_oe1_not();
  set1_enc_sel1();
  set0_enc_oe1_not();
  _delay_loop_1(10);
  uint8_t enc1l = getpin_enc_data();
  set1_enc_oe1_not();

  accum_88_32(enc1l, enc1h, enc1_r);

  set0_enc_sel2();
  set0_enc_oe2_not();
  _delay_loop_1(10);
  uint8_t enc2h = getpin_enc_data();
  set1_enc_oe2_not();
  set1_enc_sel2();
  set0_enc_oe2_not();
  _delay_loop_1(10);
  uint8_t enc2l = getpin_enc_data();
  set1_enc_oe2_not();

  accum_88_32(enc2l, enc2h, enc2_r);
}
